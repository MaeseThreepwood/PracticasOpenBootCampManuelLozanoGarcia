public class Main {
    public static void main(String[] args) {
        Coche micoche = new Coche();
        micoche.añadirpuerta();
        System.out.println("Mi coche tiene " + micoche.puertas + " puertas");
    }
}
class Coche {
            public int puertas = 3;

            public void añadirpuerta() {
                this.puertas++;

            }
        }
